{
  "name": {
    "en": "Develco/Frient Air Quality Sensor"
  },
  "class": "sensor",
  "capabilities": [
    "measure_temperature",
    "measure_humidity",
    "measure_battery",
    "measure_voc"
  ],
  "energy": {
    "batteries": [
      "AA",
      "AA"
    ]
  },
  "images": {
    "large": "{{driverAssetsPath}}/images/large.png",
    "small": "{{driverAssetsPath}}/images/small.png"
  },
  "icon": "{{driverAssetsPath}}/icon.svg",
  "pair": [
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ],
  "repair": [
    {
      "id": "repair"
    }
  ],
  "settings": [
    {
      "$extends": "ignore-reachable"
    },
    {
      "$extends": "info"
    }
  ]
}